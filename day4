class Student:
    # Class variables
    college_name = "ABC College"
    total_no_of_students = 0

    # Constructor
    def __init__(self, student_id, student_name):
        self._student_id = student_id       # Protected attribute
        self._student_name = student_name   # Protected attribute
        Student.total_no_of_students += 1   # Increase student count


class Marks(Student):
    # Constructor
    def __init__(self, student_id, student_name, marks1, marks2):
        super().__init__(student_id, student_name)  # Initialize parent attributes
        self.marks1 = marks1
        self.marks2 = marks2

    # Method to calculate average
    def calculate_average(self):
        return (self.marks1 + self.marks2) / 2

    # Method to calculate grade
    def calculate_grade(self):
        avg = self.calculate_average()
        if avg >= 90:
            return "A+"
        elif avg >= 75:
            return "A"
        elif avg >= 60:
            return "B"
        elif avg >= 40:
            return "C"
        else:
            return "F"

    # Display student info with marks and grade
    def display_info(self):
        print("College Name:", Student.college_name)
        print("Student ID:", self._student_id)
        print("Student Name:", self._student_name)
        print("Marks1:", self.marks1)
        print("Marks2:", self.marks2)
        print("Average:", self.calculate_average())
        print("Grade:", self.calculate_grade())
        print("-" * 40)


# Example usage
s1 = Marks(101, "Arijit", 85, 92)
s2 = Marks(102, "Rahul", 55, 63)

s1.display_info()
s2.display_info()

print("Total Students:", Student.total_no_of_students)